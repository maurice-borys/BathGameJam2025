[sub_resource type="GDScript" id="GDScript_od5hr"]
script/source = "extends CharacterBody2D

@onready var healthModule : Health = $HealthModule

@export var maxHealth = 100
@export var speed = 35

func _ready() -> void:
	healthModule.maxHealth = maxHealth
	healthModule.health = maxHealth
	healthModule.healthChanged.connect(healthChanged)

func _physics_process(delta: float) -> void:
	var hori = 0
	var vert = 0
	
	if Input.is_key_pressed(KEY_W):
		vert = -1
	elif Input.is_key_pressed(KEY_S):
		vert = 1
		
	if Input.is_key_pressed(KEY_A):
		hori = -1
	elif Input.is_key_pressed(KEY_D):
		hori = 1

	velocity = Vector2(hori,vert).normalized() * speed
	move_and_slide()

func healthChanged(old, new):
	print(\"Health changed to \" + str(new))
"

